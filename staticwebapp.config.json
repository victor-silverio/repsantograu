{
  "routes": [
    {
      "route": "/.github/*",
      "statusCode": 404
    },
    {
      "route": "/divulgacao_rep/*",
      "statusCode": 404
    },
    {
      "route": "/*.md",
      "statusCode": 404
    },
    {
      "route": "/requirements.txt",
      "statusCode": 404
    },
    {
      "route": "/*.py",
      "statusCode": 404
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "route": "/.well-known/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, must-revalidate",
        "Content-Type": "text/plain"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, must-revalidate"
      }
    },
    {
      "route": "/icons/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, must-revalidate"
      }
    },
    {
      "route": "/*.{png,jpg,gif,webp,css,js,ico,xml,txt,json}",
      "headers": {
        "Cache-Control": "public, max-age=31536000, must-revalidate"
      }
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-inline' 'unsafe-eval'; img-src https: data: blob:;",
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  },
  "mimeTypes": {
    ".webp": "image/webp",
    ".json": "application/json"
  }
}