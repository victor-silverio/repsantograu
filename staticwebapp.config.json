{
  "routes": [
    {
      "route": "/.github/*",
      "statusCode": 404
    },
    {
      "route": "/node_modules/*",
      "statusCode": 404
    },
    {
      "route": "/package.json",
      "statusCode": 404
    },
    {
      "route": "/package-lock.json",
      "statusCode": 404
    },
    {
      "route": "/staticwebapp.config.json",
      "statusCode": 404
    },
    {
      "route": "/.gitignore",
      "statusCode": 404
    },
    {
      "route": "/LICENSE",
      "statusCode": 404
    },
    {
      "route": "/*.yml",
      "statusCode": 404
    },
    {
      "route": "/sw.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/divulgacao_rep/*",
      "statusCode": 404
    },
    {
      "route": "/src/vagas.json",
      "statusCode": 404
    },
    {
      "route": "/src/amenities.json",
      "statusCode": 404
    },
    {
      "route": "/scripts/*",
      "statusCode": 404
    },
    {
      "route": "/*.md",
      "statusCode": 404
    },
    {
      "route": "/requirements.txt",
      "statusCode": 404
    },
    {
      "route": "/*.py",
      "statusCode": 404
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "route": "/fotos.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "route": "/.well-known/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, must-revalidate",
        "Content-Type": "text/plain"
      }
    },
    {
      "route": "/icons/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, must-revalidate"
      }
    },
    {
      "route": "/fonts/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, must-revalidate"
      }
    },
    {
      "route": "/*.{png,jpg,gif,webp,ico,xml,txt}",
      "headers": {
        "Cache-Control": "public, max-age=31536000, must-revalidate"
      }
    },
    {
      "route": "/styles.css",
      "headers": {
        "Cache-Control": "no-cache, must-revalidate"
      }
    },
    {
      "route": "/src/*",
      "headers": {
        "Cache-Control": "no-cache, must-revalidate"
      }
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src 'self'; script-src 'self' https://static.cloudflareinsights.com; connect-src 'self' https://cloudflareinsights.com https://www.repsantograu.online; style-src 'self'; font-src 'self'; img-src 'self' data: https://www.repsantograu.online; frame-ancestors 'self' https://facebook.com https://www.facebook.com; object-src 'none';",
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Permissions-Policy": "camera=(), microphone=(), geolocation=(), payment=(), usb=()",
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/imagens/*",
      "/icons/*",
      "/fonts/*",
      "/styles.css",
      "/src/*",
      "/sw.js",
      "/manifest.json",
      "/*.txt",
      "/*.xml"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  },
  "mimeTypes": {
    ".webp": "image/webp",
    ".json": "application/json"
  }
}