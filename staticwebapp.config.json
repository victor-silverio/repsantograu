{
  "routes": [
    {
      "route": "/.github/*",
      "statusCode": 404
    },
    {
      "route": "/divulgacao_rep/*",
      "statusCode": 404
    },
    {
      "route": "/*.md",
      "statusCode": 404
    },
    {
      "route": "/requirements.txt",
      "statusCode": 404
    },
    {
      "route": "/*.py",
      "statusCode": 404
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "route": "/*.{png,jpg,gif,webp,css,js,ico,xml,txt}",
      "headers": {
        "Cache-Control": "public, max-age=31536000, must-revalidate"
      }
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-inline' 'unsafe-eval'; img-src https: data: blob:;"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}